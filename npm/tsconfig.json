{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2017",
    "module": "ES2015",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "declaration": true,
    "downlevelIteration": true,
    "listEmittedFiles": true
  }
}
